{
  "id": "pdf-processing",
  "name": "PDF Processing",
  "description": "Extract and transform document data with Docling OCR",
  "icon": "file",
  "workflow": {
    "name": "PDF Processing",
    "nodes": [
      {
        "parameters": {},
        "name": "When clicking 'Test workflow'",
        "type": "n8n-nodes-base.manualTrigger",
        "typeVersion": 1,
        "position": [240, 300]
      },
      {
        "parameters": {
          "operation": "readFile",
          "fileSelector": "={{ $json.file_path }}",
          "options": {}
        },
        "name": "Read Binary File",
        "type": "n8n-nodes-base.readWriteFile",
        "typeVersion": 1,
        "position": [440, 300]
      },
      {
        "parameters": {
          "url": "http://localhost:5001/v1/convert/file",
          "sendBody": true,
          "contentType": "multipart-form-data",
          "bodyParameters": {
            "parameters": [
              {
                "parameterType": "formBinaryData",
                "name": "file",
                "inputDataFieldName": "data"
              }
            ]
          },
          "options": {}
        },
        "name": "Docling OCR",
        "type": "n8n-nodes-base.httpRequest",
        "typeVersion": 4.2,
        "position": [640, 300]
      },
      {
        "parameters": {
          "jsCode": "// Extract the markdown text from the Docling response\nconst response = $input.first().json;\nreturn [{\n  json: {\n    markdown: response.markdown || response.text || '',\n    metadata: response.metadata || {}\n  }\n}];"
        },
        "name": "Process Result",
        "type": "n8n-nodes-base.code",
        "typeVersion": 2,
        "position": [840, 300]
      }
    ],
    "connections": {
      "When clicking 'Test workflow'": {
        "main": [
          [
            {
              "node": "Read Binary File",
              "type": "main",
              "index": 0
            }
          ]
        ]
      },
      "Read Binary File": {
        "main": [
          [
            {
              "node": "Docling OCR",
              "type": "main",
              "index": 0
            }
          ]
        ]
      },
      "Docling OCR": {
        "main": [
          [
            {
              "node": "Process Result",
              "type": "main",
              "index": 0
            }
          ]
        ]
      }
    },
    "settings": {
      "executionOrder": "v1"
    }
  }
}
